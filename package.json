{
  "name": "SatVis",
  "version": "1.0.0",
  "description": "Satellite orbit visualization with CesiumJS",
  "scripts": {
    "build": "webpack --config webpack/webpack.prod.js",
    "build:dev": "webpack --config webpack/webpack.config.js",
    "build:stats": "webpack --config webpack/webpack.stats.js",
    "clean": "mkdirp dist & rimraf dist/*",
    "lint": "eslint --ext js,vue src && prettier --check \"src/**/*.{js,vue}\"",
    "lint:fix": "eslint --ext js,vue --fix src && prettier --write \"src/**/*.{js,vue}\"",
    "serve": "npm run build && http-server -c-1 dist",
    "serve:dev": "npm run build:dev && http-server -c-1 dist",
    "serve:stats": "npm run build:stats && http-server -c-1 dist",
    "start": "webpack serve --progress --config webpack/webpack.config.js",
    "start:anyhost": "webpack serve --progress --config webpack/webpack.config.js --host 0.0.0.0",
    "update-tle": "node data/tle/update-tle.js",
    "update-custom-data": "npm run update-tle && bash data/custom/update-custom-data.sh"
  },
  "author": "Florian Mauracher",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.0.1",
    "@fortawesome/free-brands-svg-icons": "^7.0.1",
    "@fortawesome/free-solid-svg-icons": "^7.0.1",
    "@fortawesome/vue-fontawesome": "^3.1.2",
    "@primevue/themes": "^4.3.9",
    "@sentry/browser": "^10.15.0",
    "cesium": "^1.133.1",
    "cesium-sensor-volumes": "github:Flowm/cesium-sensor-volumes#1.102.0",
    "dayjs": "^1.11.18",
    "pinia": "^3.0.3",
    "primevue": "^4.3.9",
    "satellite.js": "^6.0.1",
    "vue": "^3.5.22",
    "vue-multiselect": "^3.3.1",
    "vue-router": "^4.5.1",
    "vue-toastification": "^2.0.0-rc.5",
    "workbox-window": "^7.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/eslint-parser": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.36.0",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "cssnano": "^7.1.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-webpack": "^0.13.10",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vue": "^10.5.0",
    "globals": "^16.4.0",
    "html-webpack-plugin": "^5.6.4",
    "http-server": "^14.1.1",
    "mini-css-extract-plugin": "^2.9.4",
    "mkdirp": "^3.0.1",
    "postcss-loader": "^8.2.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "sass": "^1.93.2",
    "sass-loader": "^16.0.5",
    "script-loader": "^0.7.2",
    "style-loader": "^4.0.0",
    "typescript": "^5.9.2",
    "url-loader": "^4.1.1",
    "vue-eslint-parser": "^10.2.0",
    "vue-loader": "^17.4.2",
    "webpack": "^5.101.3",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1",
    "webpack-strip-pragma-loader": "^1.0.1",
    "workbox-webpack-plugin": "^7.3.0"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all",
    "not UCAndroid >1"
  ]
}
