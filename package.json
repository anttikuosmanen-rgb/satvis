{
  "name": "SatVis",
  "version": "1.0.0",
  "description": "Satellite orbit visualization with CesiumJS",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "preview": "vite preview",
    "clean": "rimraf dist",
    "lint": "eslint --ext js,vue src && prettier --check \"src/**/*.{js,vue}\"",
    "lint:fix": "eslint --ext js,vue --fix src && prettier --write \"src/**/*.{js,vue}\"",
    "serve": "npm run build && npm run preview",
    "start": "vite",
    "start:anyhost": "vite --host 0.0.0.0",
    "update-tle": "node data/tle/update-tle.js",
    "update-custom-data": "npm run update-tle && bash data/custom/update-custom-data.sh"
  },
  "author": "Florian Mauracher",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.0.1",
    "@fortawesome/free-brands-svg-icons": "^7.0.1",
    "@fortawesome/free-solid-svg-icons": "^7.0.1",
    "@fortawesome/vue-fontawesome": "^3.1.2",
    "@primevue/themes": "^4.3.9",
    "@sentry/browser": "^10.16.0",
    "cesium": "^1.133.1",
    "cesium-sensor-volumes": "github:Flowm/cesium-sensor-volumes#1.133.0",
    "dayjs": "^1.11.18",
    "pinia": "^3.0.3",
    "primevue": "^4.3.9",
    "satellite.js": "^6.0.1",
    "vue": "^3.5.22",
    "vue-multiselect": "^3.3.1",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@vite-pwa/assets-generator": "^1.0.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vue": "^10.5.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-plugin-pwa": "^1.0.3",
    "vite-plugin-static-copy": "^3.1.2",
    "vue-eslint-parser": "^10.2.0"
  }
}
